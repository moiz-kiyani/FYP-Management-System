@model IEnumerable<FYPSystem.Models.Evaluation>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_LayoutSupervior.cshtml";
}

<div class="container" style="margin-top:30px; margin-left:40px;">
    <div class="card">
        <div class="card-body">
            <h1 class="display-3" style="color:#858796"><b>Marks</b></h1>
<p>
    <a asp-action="Create" class="d-none d-sm-inline-block btn btn-sm btn-primary shadow-sm">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.StudentName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.RollNum)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ProposalMarks)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PosterMarks)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SupervisorMarks)
            </th>
            @*<th>
                @Html.DisplayNameFor(model => model.ExaminerOne)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ExaminerTwo)
            </th>*@
            <th>
                @Html.DisplayNameFor(model => model.TotalMarks)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SupervisorId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SupervisorName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SupervisorEmail)
            </th>
            @*<th>
                @Html.DisplayNameFor(model => model.EvaluatorOneName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EvaluatorTwoName)
            </th>*@
            <th>
                @Html.DisplayNameFor(model => model.Student)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.StudentName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RollNum)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ProposalMarks)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PosterMarks)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SupervisorMarks)
            </td>
            @*<td>
                @Html.DisplayFor(modelItem => item.ExaminerOne)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ExaminerTwo)
            </td>*@
            <td>
                @Html.DisplayFor(modelItem => item.TotalMarks)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SupervisorId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SupervisorName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SupervisorEmail)
            </td>
            @*<td>
                @Html.DisplayFor(modelItem => item.EvaluatorOneName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EvaluatorTwoName)
            </td>*@
            <td>
                @Html.DisplayFor(modelItem => item.Student.Id)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
</div>
</div>
</div>